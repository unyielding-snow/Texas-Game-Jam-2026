pico-8 cartridge // http://www.pico-8.com
version 43
__lua__
player = {}
player.x = 24
player.y = 24
local msg_shown = false
frame = 0

ending_1 = false
ending_2 = false

function _init()
 music(2,1000,0)	
	dtb_init(3)
	-- borrowed from https://gist.github.com/shaneriley/cae98eac6136e7293b28
	player.direction = "down"
	player.moving = false
	player.sprite = 0
	player.speed = 0.2

	player.sprites = {
  down = {
    {64, 65, 80, 81},
    {66, 67, 82, 83},
    {68, 69, 84, 85},
    {64, 65, 80, 81}
  },
  up = {
    {70, 71, 86, 87},
    {72, 73, 88, 89},
    {74, 75, 90, 91},
    {70, 71, 86, 87}
  },
  right = {
    {96, 97, 112, 113},
    {98, 99, 114, 115},
    {100, 101, 116, 117},
    {96, 97, 112, 113}
  },
  left = {
    {102, 103, 118, 119},
    {104, 105, 120, 121},
    {106, 107, 122, 123},
    {102, 103, 118, 119}
  }
}
end

function _update()
	dtb_update()
-- player movement --
	player.moving = false
	local x = player.x
 local y = player.y

	frame += 1
	has_moved = false

  if btn(0) then
    x -= player.speed
    move("left")
    has_moved = true
    move_time = time()
  end
  if btn(1) then
    x += player.speed
    move("right")
    has_moved = true
    move_time = time()
  end
  if btn(2) then
    y -= player.speed
    move("up")
    has_moved = true	
    move_time = time()
  end
  if btn(3) then
    y += player.speed
    move("down")
    has_moved = true
    move_time = time()
  end
  
  if has_moved and frame > 10 then
    sfx(15)
    frame = 0
  elseif not has_moved then 
    sfx(-1)	
  end
  
 	local is_solid, is_interactive, interactive_tile_id, is_pick_up, pick_up_tile_id, pick_up_x, pick_up_y = hit(x, y, 16, 16)
		
		if not is_solid then
    player.x = x
    player.y = y
    if not msg_shown and flr(player.x) == 29 and flr(player.y) == 25 then
    	dtb_disp("maybe i didn't lose her..")
    	msg_shown = true
    	_dtb_c()
    end
    if not msg_shown and flr(player.x) == 26 and flr(player.y) == 27 then
    	dtb_disp("it feels like i'm drifting...")
    	msg_shown = true
    	_dtb_c()
    end
    if is_pick_up then
    	item_id = mget(pick_up_x, pick_up_y)
   		if item_id == 54 then
	    	dtb_disp("this picture... i think she was looking at me, her smile is so bright")
	    	mset(pick_up_x ,pick_up_y, 1)
	  			--load("ending_1.p8")
	  		elseif not ending_1 then
	  			if item_id == 192 or item_id == 193 or item_id == 208 or item_id == 209 then
	  				dtb_disp("this desk...")
	  				
	  				dtb_disp("i remember. everything now.")
	  				
	  				dtb_disp("this desk...", function()
	  					load("true_ending.p8")
	  				end)

	  				ending_1 = true
	  			end
	  		end
	  		if not ending_2 then
	  			if item_id == 016 then
	  				dtb_disp("wait...")
	  				dtb_disp("this picture...")
							dtb_disp("it's me, but why am i in a coffin?")
							dtb_disp("wait...", function()
								load("ending_1.p8")
							end)
	  				ending_2 = true
	  			end
	  		end
  		end
  else
    if is_interactive and not msg_shown then
      local msg = get_interactive_message(interactive_tile_id)
      --dtb_disp(msg)
      msg_shown = true
    end
  end
  
  if msg_shown then
			if btnp(4) then 
				_dtb_c()
				msg_shown = false 
			end
	end

  if has_moved and time() - move_time >= 1 then
    sfx(15)
    has_moved = false
  end
end


function _draw()
	cls()

	room_x = flr(player.x/16)
	room_y = flr(player.y/16)
	camera(room_x*128,room_y*128)

	-- draw the whole map (128⁙32)
	map()
	
	local frame = player.sprites[player.direction][player.sprite + 1]
	spr(frame[1], player.x*8, player.y*8)
	spr(frame[2], player.x*8 + 8, player.y*8)
	spr(frame[3], player.x*8, player.y*8 + 8)
	spr(frame[4], player.x*8 + 8, player.y*8 + 8)
	
	camera()
	dtb_draw()
	pal()
end

-- borrowed from https://gamedev.docrobs.co.uk/first-steps-in-pico-8-easy-collisions-with-map-tiles
function hit(x, y, w, h)
	local is_solid = false
	local is_interactive = false
	local interactive_tile_id = nil
 local is_pickup = false
 local pick_up_tile_id = nil
	local pick_up_x = nil
	local pick_up_y = nil
 
  for i = x*8, x*8 + w - 1, 6 do
    for j = y*8, y*8 + h - 1, 6 do
      local tile_x = flr(i / 8)
      local tile_y = flr(j / 8)
      local tile_id = mget(tile_x, tile_y)
      if fget(tile_id, 0) then  -- check flag 0
        is_solid = true
      end
      if fget(tile_id, 1) then
        is_interactive = true
        interactive_tile_id = tile_id
      end
      if fget(tile_id, 2) then
      		pick_up_tile_id = tile_id
        is_pick_up = true
        pick_up_x = tile_x
        pick_up_y = tile_y
      end
    end
  end
  return is_solid, is_interactive, interactive_tile_id, pick_up_tile_id, is_pick_up, pick_up_x, pick_up_y
end


function move(dir)
  player.moving = true
  player.direction = dir
  player.sprite = (player.sprite + 1) % #player.sprites[dir]
end

function get_interactive_message(tile_id)
	if tile_id == 38 then -- bed
		return "it's a cozy bed.. smells like warm tea leaves after the rain 🐱"
	elseif tile_id == 40 then --bookshelf
		return "her favorite romance novels, spines worn from the nights she would read to me"
	elseif tile_id == 18 then --drawer
		return "her clutter makes me want to sit here, like she'll walk in at any second."
	elseif tile_id == 54 then --photo
		return "♥"
	else
		return "smells funny in here...."
	end
end

-- borrowed from https://www.lexaloffle.com/bbs/?tid=28465
-- dialogue text box library by oli414. minimized.
function dtb_init(n) dtb_q={}dtb_f={}dtb_n=3 if n then dtb_n=n end _dtb_c() end function dtb_disp(t,c)local s,l,w,h,u s={}l=""w=""h=""u=function()if #w+#l>29 then add(s,l)l=""end l=l..w w=""end for i=1,#t do h=sub(t,i,i)w=w..h if h==" "then u()elseif #w>28 then w=w.."-"u()end end u()if l~=""then add(s,l)end add(dtb_q,s)if c==nil then c=0 end add(dtb_f,c)end function _dtb_c()dtb_d={}for i=1,dtb_n do add(dtb_d,"")end dtb_c=0 dtb_l=0 end function _dtb_l()dtb_c+=1 for i=1,#dtb_d-1 do dtb_d[i]=dtb_d[i+1]end dtb_d[#dtb_d]=""sfx(2)end function dtb_update()if #dtb_q>0 then if dtb_c==0 then dtb_c=1 end local z,x,q,c z=#dtb_d x=dtb_q[1]q=#dtb_d[z]c=q>=#x[dtb_c]if c and dtb_c>=#x then if btnp(4) then if dtb_f[1]~=0 then dtb_f[1]()end del(dtb_f,dtb_f[1])del(dtb_q,dtb_q[1])_dtb_c()sfx(2)return end elseif dtb_c>0 then dtb_l-=1 if not c then if dtb_l<=0 then local v,h v=q+1 h=sub(x[dtb_c],v,v)dtb_l=1 if h~=" " then sfx(0)end if h=="." then dtb_l=6 end dtb_d[z]=dtb_d[z]..h end if btnp(4) then dtb_d[z]=x[dtb_c]end else if btnp(4) then _dtb_l()end end end end end function dtb_draw()if #dtb_q>0 then local z,o z=#dtb_d o=0 if dtb_c<z then o=z-dtb_c end rectfill(2,125-z*8,125,125,0)if dtb_c>0 and #dtb_d[#dtb_d]==#dtb_q[1][dtb_c] then print("\x8e",118,120,1)end for i=1,z do print(dtb_d[i],4,i*8+119-(z+o)*8,7)end end end

