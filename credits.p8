bar_x = -8       -- starting x pos of first bar
bar_w = 8        -- width of each bar
bar_speed = 2    -- how many pixels per frame it moves

said_dialogue = false
cutscene = 1

function _init()
				--music(3,1000)
				dtb_init()
		
    -- start black
    cls(0)
end

function _update()
    -- move bars across screen
    bar_x += bar_speed

    -- if finished bar, allow dialogue

    if bar_x > 128 then
        if not said_dialogue then
            said_dialogue = true
            -- Frame One
            music(3,1000)
        				dtb_disp("\"♥i, we're team bwthhybl's\"")
        			 dtb_disp("(bella where the hell have you been loca's)")
        			 dtb_disp("thank you for playing our game!")
        			 dtb_disp("∧e app➡️eciate all feedback!")
        			 dtb_disp("this is the first game jam for 90% of us.")
        			 dtb_disp("there are two endings in this game, triggered by random objects.")
        			 dtb_disp("♥ope y🅾️u ♥ad ✽un!")        			 
           	dtb_disp("leave a commnet on our itch.io page!")
       					dtb_disp("bye bye!", function()
       					 stop("thanks for playing")
       					end)
        end
   	end 
   	   
    dtb_update()
end

function _draw()
    if cutscene == 1 then
        map(0,0,0,0,16,16) -- draw tilemap
    elseif cutscene == 2 then
        map(16,0,0,0,16,16)
    end

	for i=0,16 do
        rectfill(bar_x + i*bar_w, 0,
                 bar_x + i*bar_w + bar_w-1, 127, 0)
    end
    
    dtb_draw()
end

-- smooth fade-in: gradually restore colors
function fadein()
    fade_step = fade_step + 1
    if fade_step % fadespeed == 0 then
        local t = fade_step // fadespeed
        for i=0,15 do
            -- interpolate from black (0) to normal color i
            local col = i
            local mix = flr((t / fade_max) * col)
            pal(i, mix, 1)
        end
    end

    if fade_step >= fade_max * fadespeed then
        pal()      -- restore normal palette
        fading = 0 -- fade complete
    end
end

function switch_cutscene()
    bar_x = -8       -- starting x pos of first bar
    bar_w = 8        -- width of each bar  
    cutscene = 2 
end

function ending()
    load("credits.p8")
end

