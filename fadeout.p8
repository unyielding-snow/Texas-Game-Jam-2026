start_fade = false
text_num = 0

-- fade to black example with tilemap
-- based on dw817's demo

function _init()
		dtb_init()
				
  pal(0,128,1) -- force disrupt
  pal()

  fading=0
  fadespeed=5

  cls(0)

  -- optional: draw your tilemap initially
  -- we will use a 16x16 tilemap (128x128 pixels)
  map(0,0,0,0,16,16)
end

function _draw()
  -- always draw the tilemap each frame
  map(0,0,0,0,16,16)

  if (fading>0) fadeout()
  
  if fading<0 then
    print("complete!",46,62,7)
    load("floor.p8")
				run()
  end
  
  dtb_draw()
end

function _update()
 	dtb_update()
 	
 	if text_num == 0 then
			dtb_disp("   ★press c to remember★")
	
			dtb_disp("it used to be...", function()
				music(5)
			end)
			
			dtb_disp("just the two of us.")
			
			dtb_disp("golden like a rusted locket")
			
			dtb_disp("until everything changed...")
	
			dtb_disp("you must go back and collect all the memories")
	
	 	dtb_disp("before everything fades",function()
	    --whatever is in this function is called after this dialogue is done.
	    start_fadeout()
			end)
			text_num += 1
		end

  if start_fade and fading==0 then
    fadeout()
  end
end

-- function to fade screen (palette shift)
-- exactly the same as dw817
function fadeout()
  local fade,c,p={[0]=0,17,18,19,20,16,22,6,24,25,9,27,28,29,29,31,0,0,16,17,16,16,5,0,2,4,0,3,1,18,2,4}
  fading+=1
  if fading%fadespeed==1 then
    for i=0,15 do
      c=peek(24336+i)
      if (c>=128) c-=112
      p=fade[c]
      if (p>=16) p+=112
      pal(i,p,1)
    end
    if fading==7*fadespeed+1 then
      cls()
      pal()
      fading=-1
    end
  end
end

function start_fadeout()
	start_fade = true
end
