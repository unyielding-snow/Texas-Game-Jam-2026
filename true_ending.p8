bar_x = -8       -- starting x pos of first bar
bar_w = 8        -- width of each bar
bar_speed = 2    -- how many pixels per frame it moves

said_dialogue = false

function _init()
				--music(3,1000)
				dtb_init()
		
    -- start black
    cls(0)
end

function _update()
    -- move bars across screen
    bar_x += bar_speed

    -- if finished bar, allow dialogue
    if bar_x > 128 then
    			if not said_dialogue then
      			said_dialogue = true
      			dtb_disp("it's great seeing you again, raya!", function()
      				music(3,1000)
      			end)
      			dtb_disp("just the two of us.")
   			   dtb_disp("deep down we both know the truth, atma")
   			   dtb_disp("what?")
   			   dtb_disp("we are dead, ash.")
   				end
    end
    
    dtb_update()
end

function _draw()
    map(0,0,0,0,16,16) -- draw tilemap

				for i=0,16 do
        rectfill(bar_x + i*bar_w, 0,
                 bar_x + i*bar_w + bar_w-1, 127, 0)
    end
    
    dtb_draw()
end

-- smooth fade-in: gradually restore colors
function fadein()
    fade_step = fade_step + 1
    if fade_step % fadespeed == 0 then
        local t = fade_step // fadespeed
        for i=0,15 do
            -- interpolate from black (0) to normal color i
            local col = i
            local mix = flr((t / fade_max) * col)
            pal(i, mix, 1)
        end
    end

    if fade_step >= fade_max * fadespeed then
        pal()      -- restore normal palette
        fading = 0 -- fade complete
    end
end

