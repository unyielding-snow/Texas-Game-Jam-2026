-- slow fade-in from black
fading = 0
fadespeed = 10   -- higher = slower
fade_step = 0
fade_max = 150   -- more steps = slower/more gradual

mask_radius = 0
mask_max = 50

function _init()
	   dtb_init()
	   	   
	   begin_fill()
	   
    -- start black
    cls(0)
    for i=0,15 do pal(i,0,1) end
    fading = 1
end

function _update()
   	dtb_update()

    if fading > 0 then
        fadein()
    end
end

event_time = time()
fill_circle = false
once = true
twice = true
start = time()

function _draw()
	if fill_circle then
				mask_radius = mask_radius + 1
    -- draw black rectangle
    rectfill(0,0,127,127,0)
    -- cut out circle
    circfill(64,64,mask_radius,7) -- color doesn't matter if using pal
	end	
	
	if twice and time() - start >= 1 then
			music(1)
			twice = false
	end
	 
	if once and time() - event_time >= 3 then
		once = false

		dtb_disp("no...")
		dtb_disp("â§—his câ–ˆnt be.")
		dtb_disp("''â¬‡ï¸on't you know, hero?''")		
		dtb_disp("''uou're are already dead.''")
		dtb_disp("''stop clinging onto these memories.''")
		dtb_disp("âˆ§e have already gðŸ…¾ï¸ne extinct.")
		dtb_disp("accept your fate and die.")
		dtb_disp("hope you can find our memories in your next life.")
		dtb_disp("this is goodbye.", function()
			load("credits.p8")
		end)
	end	

 map(0,0,0,0,16,16) -- draw tilemap

 dtb_draw()
end

function begin_fill()
	event_time = time()
	fill_circle = true
	fading = 1
	fade_step = 0
end

-- smooth fade-in: gradually restore colors
function fadein()
    fade_step = fade_step + 1
    if fade_step % fadespeed == 0 then
        local t = fade_step // fadespeed
        for i=0,15 do
            -- interpolate from black (0) to normal color i
            local col = i
            local mix = flr((t / fade_max) * col)
            pal(i, mix, 1)
        end
    end

    if fade_step >= fade_max * fadespeed then
        pal()      -- restore normal palette
        fading = 0 -- fade complete
    end
end

